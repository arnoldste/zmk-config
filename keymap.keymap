/ {
  keymap {
    compatible = "zmk,keymap";

    default_layer {
      bindings = <
        /* Joystick */
        &kp UP        /* Joystick UP */
        &kp DOWN      /* Joystick DOWN */
        &kp LEFT      /* Joystick LEFT */
        &kp RIGHT     /* Joystick RIGHT */
        &kp ENTER     /* Joystick PRESS */

        /* Buttons 1â€“6 */
        &kp N1
        &kp N2
        &kp N3
        &kp N4
        &kp N5
        &kp N6
      >;
    };
  };
};
